# Environment Variables for Location Tracker App
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=3000
MAX_HISTORY=100

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/google/maps-apis/
# Enable these APIs: Maps JavaScript API, Places API (if needed)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Optional: Debug mode for Socket.IO
DEBUG=false